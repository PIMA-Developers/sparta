{% doc %}
  @prompt
    Quero uma section que contenha imagens espalhadas dentro da section. Cada uma contendo uma animação leve se movimentando constantemente. Quero que seja possível acrescentar até 12 imagens. Quero que seja possível aplicar ou não blur nas imagens, definir aspect radio e tamanho de cada imagem, border radius. Quando a imagem receber a propriedade hover, ela terá uma animação aumentando o tamanho da imagem e retirando o blur, caso tenha sido aplciado.
    Na versão mobile, quero que seja apresentado como carrossel simples., Quero que seja possível definir o aspect radio de cada imagem. Também tenha uma opção que a imagem já esteja ou não com efeito blur nativo. Quero que acrescente uma sombra bem leve customizável quando a imagem receber a propriedade hover para dar uma sensação de profundidade.
{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-floating-images-{{ ai_gen_id }} {
    position: relative;
    width: 100%;
    min-height: {{ block.settings.section_height }}px;
    overflow: hidden;
    padding: 40px 20px;
  }

  .ai-floating-images-container-{{ ai_gen_id }} {
    position: relative;
    width: 100%;
    height: 100%;
  }

  .ai-floating-image-{{ ai_gen_id }} {
    position: absolute;
    cursor: pointer;
    transition: all 0.3s ease;
    border-radius: {{ block.settings.border_radius }}px;
    overflow: hidden;
  }

  .ai-floating-image-{{ ai_gen_id }} img {
    width: 100%;
    height: 100%;
    object-fit: cover;
    transition: all 0.3s ease;
    border-radius: {{ block.settings.border_radius }}px;
  }

  .ai-floating-image-{{ ai_gen_id }}:hover {
    transform: scale({{ block.settings.hover_scale }});
    box-shadow: {{ block.settings.hover_shadow_x }}px {{ block.settings.hover_shadow_y }}px {{ block.settings.hover_shadow_blur }}px rgba(0, 0, 0, {{ block.settings.hover_shadow_opacity }});
    z-index: 10;
  }

  .ai-floating-image-{{ ai_gen_id }}:hover img {
    filter: blur(0px) !important;
  }

  .ai-floating-image-placeholder-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    background-color: #f4f4f4;
    display: flex;
    align-items: center;
    justify-content: center;
    border-radius: {{ block.settings.border_radius }}px;
  }

  .ai-floating-image-placeholder-{{ ai_gen_id }} svg {
    width: 60%;
    height: 60%;
    opacity: 0.3;
  }

  @media screen and (max-width: 749px) {
    .ai-floating-images-{{ ai_gen_id }} {
      padding: 20px 0;
    }

    .ai-floating-images-container-{{ ai_gen_id }} {
      display: none;
    }

    .ai-floating-carousel-{{ ai_gen_id }} {
      display: block;
      position: relative;
      width: 100%;
      overflow: hidden;
    }

    .ai-floating-carousel-track-{{ ai_gen_id }} {
      display: flex;
      transition: transform 0.3s ease;
      gap: 16px;
      padding: 0 20px;
    }

    .ai-floating-carousel-slide-{{ ai_gen_id }} {
      flex: 0 0 auto;
      border-radius: {{ block.settings.border_radius }}px;
      overflow: hidden;
    }

    .ai-floating-carousel-slide-{{ ai_gen_id }} img {
      width: 100%;
      height: 100%;
      object-fit: cover;
      border-radius: {{ block.settings.border_radius }}px;
    }

    .ai-floating-carousel-controls-{{ ai_gen_id }} {
      display: flex;
      justify-content: center;
      gap: 8px;
      margin-top: 20px;
    }

    .ai-floating-carousel-dot-{{ ai_gen_id }} {
      width: 8px;
      height: 8px;
      border-radius: 50%;
      background-color: rgba(0, 0, 0, 0.3);
      cursor: pointer;
      transition: background-color 0.3s ease;
    }

    .ai-floating-carousel-dot-{{ ai_gen_id }}.active {
      background-color: rgba(0, 0, 0, 0.8);
    }
  }

  @media screen and (min-width: 750px) {
    .ai-floating-carousel-{{ ai_gen_id }} {
      display: none;
    }
  }
{% endstyle %}

<floating-images-{{ ai_gen_id }}
  class="ai-floating-images-{{ ai_gen_id }}"
  {{ block.shopify_attributes }}
>
  <div class="ai-floating-images-container-{{ ai_gen_id }}">
    {% for i in (1..12) %}
      {% liquid
        assign image_key = 'image_' | append: i
        assign size_key = 'image_' | append: i | append: '_size'
        assign aspect_key = 'image_' | append: i | append: '_aspect'
        assign blur_key = 'image_' | append: i | append: '_blur'
        assign x_key = 'image_' | append: i | append: '_x'
        assign y_key = 'image_' | append: i | append: '_y'

        assign image = block.settings[image_key]
        assign size = block.settings[size_key]
        assign aspect = block.settings[aspect_key]
        assign blur = block.settings[blur_key]
        assign x_pos = block.settings[x_key]
        assign y_pos = block.settings[y_key]
      %}

      {% if image != blank %}
        {% liquid
          case aspect
            when 'square'
              assign width = size
              assign height = size
            when 'portrait'
              assign width = size
              assign height = size | times: 1.4 | round
            when 'landscape'
              assign width = size | times: 1.4 | round
              assign height = size
          endcase
        %}

        <div
          class="ai-floating-image-{{ ai_gen_id }} ai-floating-image-{{ ai_gen_id }}-{{ i }}"
          style="
            width: {{ width }}px;
            height: {{ height }}px;
            left: {{ x_pos }}%;
            top: {{ y_pos }}%;
            transform: translate(-50%, -50%);
          "
          data-image-index="{{ i }}"
        >
          <img
            src="{{ image | image_url: width: 400 }}"
            alt="{{ image.alt | escape }}"
            loading="lazy"
            width="{{ width }}"
            height="{{ height }}"
            style="filter: blur({{ blur }}px);"
          >
        </div>
      {% endif %}
    {% endfor %}
  </div>

  <div class="ai-floating-carousel-{{ ai_gen_id }}">
    <div class="ai-floating-carousel-track-{{ ai_gen_id }}">
      {% for i in (1..12) %}
        {% liquid
          assign image_key = 'image_' | append: i
          assign mobile_size_key = 'image_' | append: i | append: '_mobile_size'
          assign mobile_aspect_key = 'image_' | append: i | append: '_mobile_aspect'
          assign blur_key = 'image_' | append: i | append: '_blur'

          assign image = block.settings[image_key]
          assign mobile_size = block.settings[mobile_size_key]
          assign mobile_aspect = block.settings[mobile_aspect_key]
          assign blur = block.settings[blur_key]
        %}

        {% if image != blank %}
          {% liquid
            case mobile_aspect
              when 'square'
                assign mobile_width = mobile_size
                assign mobile_height = mobile_size
              when 'portrait'
                assign mobile_width = mobile_size
                assign mobile_height = mobile_size | times: 1.4 | round
              when 'landscape'
                assign mobile_width = mobile_size | times: 1.4 | round
                assign mobile_height = mobile_size
            endcase
          %}

          <div
            class="ai-floating-carousel-slide-{{ ai_gen_id }}"
            style="
              width: {{ mobile_width }}px;
              height: {{ mobile_height }}px;
            "
          >
            {% if image %}
              <img
                src="{{ image | image_url: width: 300 }}"
                alt="{{ image.alt | escape }}"
                loading="lazy"
                width="{{ mobile_width }}"
                height="{{ mobile_height }}"
                style="filter: blur({{ blur }}px);"
              >
            {% else %}
              <div class="ai-floating-image-placeholder-{{ ai_gen_id }}">
                {{ 'image' | placeholder_svg_tag }}
              </div>
            {% endif %}
          </div>
        {% endif %}
      {% endfor %}
    </div>

    <div class="ai-floating-carousel-controls-{{ ai_gen_id }}">
      {% assign visible_images = 0 %}
      {% for i in (1..12) %}
        {% liquid
          assign image_key = 'image_' | append: i
          assign image = block.settings[image_key]
        %}
        {% if image != blank %}
          {% assign visible_images = visible_images | plus: 1 %}
        {% endif %}
      {% endfor %}

      {% for i in (1..visible_images) %}
        <div class="ai-floating-carousel-dot-{{ ai_gen_id }}" data-slide="{{ forloop.index0 }}"></div>
      {% endfor %}
    </div>
  </div>
</floating-images-{{ ai_gen_id }}>

<script>
  (function() {
    class FloatingImages{{ ai_gen_id }} extends HTMLElement {
      constructor() {
        super();
        this.currentSlide = 0;
        this.totalSlides = 0;
        this.autoplayInterval = null;
      }

      connectedCallback() {
        this.initFloatingAnimation();
        this.initCarousel();
      }

      initFloatingAnimation() {
        const images = this.querySelectorAll('.ai-floating-image-{{ ai_gen_id }}');
        
        images.forEach((image, index) => {
          const animationDelay = index * 0.5;
          const animationDuration = 3 + (index % 3);
          
          image.style.animationDelay = `${animationDelay}s`;
          image.style.animation = `ai-floating-move-${index % 3} ${animationDuration}s ease-in-out infinite alternate`;
        });

        this.createFloatingKeyframes();
      }

      createFloatingKeyframes() {
        const style = document.createElement('style');
        style.textContent = `
          @keyframes ai-floating-move-0 {
            0% { transform: translate(-50%, -50%) translateY(0px) translateX(0px); }
            100% { transform: translate(-50%, -50%) translateY(-10px) translateX(5px); }
          }
          @keyframes ai-floating-move-1 {
            0% { transform: translate(-50%, -50%) translateY(0px) translateX(0px); }
            100% { transform: translate(-50%, -50%) translateY(8px) translateX(-3px); }
          }
          @keyframes ai-floating-move-2 {
            0% { transform: translate(-50%, -50%) translateY(0px) translateX(0px); }
            100% { transform: translate(-50%, -50%) translateY(-6px) translateX(8px); }
          }
        `;
        document.head.appendChild(style);
      }

      initCarousel() {
        const track = this.querySelector('.ai-floating-carousel-track-{{ ai_gen_id }}');
        const dots = this.querySelectorAll('.ai-floating-carousel-dot-{{ ai_gen_id }}');
        const slides = this.querySelectorAll('.ai-floating-carousel-slide-{{ ai_gen_id }}');
        
        if (!track || dots.length === 0) return;

        this.totalSlides = dots.length;
        
        if (this.totalSlides <= 1) return;

        dots.forEach((dot, index) => {
          dot.addEventListener('click', () => {
            this.goToSlide(index);
          });
        });

        this.updateCarousel();
        this.startAutoplay();

        track.addEventListener('touchstart', (e) => {
          this.touchStartX = e.touches[0].clientX;
        });

        track.addEventListener('touchend', (e) => {
          if (!this.touchStartX) return;
          
          const touchEndX = e.changedTouches[0].clientX;
          const diff = this.touchStartX - touchEndX;
          
          if (Math.abs(diff) > 50) {
            if (diff > 0) {
              this.nextSlide();
            } else {
              this.prevSlide();
            }
          }
          
          this.touchStartX = null;
        });
      }

      goToSlide(index) {
        this.currentSlide = index;
        this.updateCarousel();
        this.restartAutoplay();
      }

      nextSlide() {
        this.currentSlide = (this.currentSlide + 1) % this.totalSlides;
        this.updateCarousel();
      }

      prevSlide() {
        this.currentSlide = (this.currentSlide - 1 + this.totalSlides) % this.totalSlides;
        this.updateCarousel();
      }

      updateCarousel() {
        const track = this.querySelector('.ai-floating-carousel-track-{{ ai_gen_id }}');
        const dots = this.querySelectorAll('.ai-floating-carousel-dot-{{ ai_gen_id }}');
        const slides = this.querySelectorAll('.ai-floating-carousel-slide-{{ ai_gen_id }}');
        
        if (!track || !slides.length) return;

        const slideWidth = slides[0].offsetWidth + 16;
        const offset = -this.currentSlide * slideWidth;
        
        track.style.transform = `translateX(${offset}px)`;

        dots.forEach((dot, index) => {
          dot.classList.toggle('active', index === this.currentSlide);
        });
      }

      startAutoplay() {
        if (this.totalSlides <= 1) return;
        
        this.autoplayInterval = setInterval(() => {
          this.nextSlide();
        }, 4000);
      }

      restartAutoplay() {
        if (this.autoplayInterval) {
          clearInterval(this.autoplayInterval);
        }
        this.startAutoplay();
      }

      disconnectedCallback() {
        if (this.autoplayInterval) {
          clearInterval(this.autoplayInterval);
        }
      }
    }

    customElements.define('floating-images-{{ ai_gen_id }}', FloatingImages{{ ai_gen_id }});
  })();
</script>

