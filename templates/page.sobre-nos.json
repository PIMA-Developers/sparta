            "text_PxyWJa": {
              "type": "text",
              "name": "t:names.text",
              "settings": {
                "text": "<p><strong>Nossa Missão</strong></p>",
                "width": "100%",
                "max_width": "normal",
                "alignment": "center",
                "type_preset": "custom",
                "font": "var(--font-body--family)",
                "font_size": "0.875rem",
                "line_height": "loose",
                "letter_spacing": "loose",
                "case": "uppercase",
                "wrap": "pretty",
                "color": "var(--color-foreground-heading)",
                "background": false,
                "background_color": "#00000026",
                "corner_radius": 0,
                "padding-block-start": 0,
